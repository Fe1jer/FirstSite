@using Microsoft.AspNetCore.Identity
@model List<IdentityRole<int>>
@{
    ViewData["Title"] = "Список ролей";
}
<h2>@ViewData["Title"]</h2>
<table class="table">
    @foreach (var role in Model)
    {
        <tr>
            <td>@role.Name</td>
        </tr>
    }
</table>
<a asp-action="UserList">Список пользователей</a>